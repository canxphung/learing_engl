{
  "name": "website-learn",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "apps/*",
    "lib/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start:user-service:dev": "cd apps/user-service; ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/main.ts",
    "start:user-service": "node dist/apps/user-service/main.js",
    "start:auth:dev": "cd apps/auth; ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/main.ts",
    "start:auth": "node dist/apps/auth/main.js",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "proto:gen": "npx buf generate --template buf.gen.yaml protos"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.59.0",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-proto": "^2.8.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.8",
    "@nestjs/core": "^10.4.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/typeorm": "^10.0.2",
    "class-validator": "^0.14.2",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27"
  }
}
